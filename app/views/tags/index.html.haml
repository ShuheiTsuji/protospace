/ .container
/   .row
/     .tags_list
/       %h2 Tags List
/       %ul.proto-tag-list.list-inline
/         %li
.jumbotron
  .container.text-center
    = image_tag("header_logo.png", alt: "PROTO TYPE - SHARE THE PROTOTYPE", height: "73px")
.container
  .row
  %h2
    - @tags.each do |tag|
      %ul.col-md-4
        = link_to tag.name, tag_path(tag.id), class: 'btn btn-defaults'
